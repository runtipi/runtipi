  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@hey-api/openapi-ts": "^0.56.1",
    "@playwright/test": "^1.49.0",
    "@types/pg": "^8.11.10",
    "dotenv-cli": "^7.4.3",
    "turbo": "^2.3.0"
  },
  "dependencies": {
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.36.3",
    "pg": "^8.13.1",
    "zod": "^3.23.8"
  }
